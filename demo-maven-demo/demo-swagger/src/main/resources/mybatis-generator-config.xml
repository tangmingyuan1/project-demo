<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!--mysql的连接包  在左边项目下面External Libraries 下搜索mysql-->
    <classPathEntry location="D:\tool\maven\repository3.8.4\mysql\mysql-connector-java\8.0.20\mysql-connector-java-8.0.20.jar" />
    <!--连接数据源 因为我用的是最新版本的数据源com.mysql.cj.jdbc.Driver 之前没有cj 所以得添加serverTimezone=UTC -->
    <context id="DB2Tables" targetRuntime="MyBatis3">

        <!-- 是否生成注释 去除自动生成的注释-->
        <commentGenerator>
            <property name="suppressDate" value="true"/>

            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!-- 数据库连接配置 -->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/apolloportaldb?serverTimezone=UTC"
                        userId="root"
                        password="root">
        </jdbcConnection>


        <!--实体类-->
        <javaModelGenerator targetPackage="com.swagger.entity"
                            targetProject="${user.dir}/src/main/java"/>
        <!--映射文件生成器  xml-->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="${user.dir}/src/main/resources"/>
        <!--接口生成器-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.swagger.mapper"
                             targetProject="${user.dir}/src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        <!--表映射-->
        <table  tableName="consumerrole"

                enableCountByExample="false"
                enableDeleteByExample="false"
                enableSelectByExample="false"
                enableUpdateByExample="false">
            <!-- enableUpdateByExample="false" 这样是不生成example -->
            <!--            <property name="useActualColumnNames" value="true"/>-->
            <!--            <generatedKey column="id" sqlStatement="mysql" identity="true" />-->
            <!--            <columnOverride column="name" property="name" />-->
            <!--            <columnOverride column="age" property="age" />-->
        </table>

    </context>
</generatorConfiguration>